<testCaseList>
        <htmlTabRef>rfsim-4gnr</htmlTabRef>
        <htmlTabName>Testing 4G NR RF sim in containers</htmlTabName>
        <htmlTabIcon>wrench</htmlTabIcon>
        <repeatCount>4</repeatCount>
        <TestCaseRequestedList>
 100011
 000011
 000001
 000012
 000002
